{
    "editor.fontSize": 15,
    "editor.quickSuggestions": {
        "other": true,
        "comments": false,
        // This enables autocomplete similar to default Sublime Text behaviour
        "strings": true
    },
    "editor.rulers": [
        120
    ],
    "files.associations": {
        "*.conf.j2": "properties",
        "*.json.j2": "json",
        "*.properties.j2": "properties",
        "*.service": "properties",
        "*.service.j2": "properties",
        "*.sh.j2": "shellscript",
        "*.xml.j2": "xml"
    },
    "files.trimTrailingWhitespace": true,
    "workbench.colorTheme": "Solarized Dark High Contrast",
    // This will disable the icons in the file explorer sidebar
    "workbench.iconTheme": null
}